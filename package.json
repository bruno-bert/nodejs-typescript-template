{
  "name": "nodejs-clean-architecture",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node main/server.js",
    "dev": "tsx --watch src/main/server.ts",
    "build": "rimraf dist && tsup src",
    "test": "vitest",
    "test:staged": "vitest run",
    "coverage": "vitest run --coverage",
    "lint-staged": "lint-staged",
    "lint": "eslint src/** --fix",
    "docker:build": "docker build -t my-mongodb .",
    "docker:run": "docker run -d --name my-mongodb-container -p 27017:27017 my-mongodb",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@ngneat/falso": "^7.2.0",
    "@rocketseat/eslint-config": "^2.2.2",
    "@shelf/jest-mongodb": "^4.2.0",
    "@types/express": "^4.17.21",
    "@types/faker": "^6.6.9",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.11.28",
    "@vitest/coverage-istanbul": "^1.4.0",
    "@vitest/coverage-v8": "^1.4.0",
    "bson-objectid": "^2.0.4",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0",
    "vitest-mongodb": "^0.0.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "mongo-round": "^1.0.0",
    "mongodb": "^6.5.0"
